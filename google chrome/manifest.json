{
"manifest_version": 3,
"name": "Keep it flow.",
"version":"1.0",
"background":{
"service_worker": "background.js"
},
"description":"This extension will help you focus while managing the sounds in the background.",
"icons":{
 "128":"icons/icon128.png",
 "48" : "icons/icon48.png",
  "16" : "icons/icon16.png"
},
"action":{

"default_icon":"icons/icon16.png",
"default_popup":"popup.html"

},"permissions": [
    "tabs",
    "storage"
  ]



}